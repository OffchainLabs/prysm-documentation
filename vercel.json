{
  "buildCommand": "npx docusaurus build --out-dir build/prysm/docs && node scripts/postbuild.js",
  "outputDirectory": "build",
  "framework": "docusaurus",
  "installCommand": "npm install",
  "trailingSlash": true,
  "cleanUrls": true,
  "redirects": [
    { "source": "/", "destination": "/prysm/docs/", "permanent": true },
    { "source": "/prysm", "destination": "/prysm/docs/", "permanent": true },
    { "source": "/prysm/docs", "destination": "/prysm/docs/", "permanent": true },
    { "source": "/prysm/docs/getting-started/", "destination": "/prysm/docs/", "permanent": true }
  ]
}
